    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/_build/nrf52810_xxaa.out",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "arm-none-eabi-gdb",
            "debugServerPath": "JLinkGDBServer",
            "debugServerArgs": "-select USB -device nRF52810_xxAA -endian little -if SWD -speed auto -noir -LocalhostOnly",
            "serverStarted": "Connecting to target...Connected to target",
            "filterStderr": true,
            "filterStdout": true,
            "setupCommands": [
                { "text": "target extended-remote localhost:2331" },
                { "text": "load ${workspaceFolder}/_build/nrf52810_xxaa.out" },
                { "text": "monitor reset" }
            ]
        }
    ]
}
